@model TeamGlobal.Infrastructure.ViewModel.QueryViewModel

<div class="form-group  has-feedback" id="divFirstName">
    <label class="control-label col-sm-4" for="firstName">First Name:</label>
    <div class="col-sm-8">
        <input type="text" class="form-control" id="firstName" maxlength="20" placeholder="First Name" name="firstName">
        <span class="glyphicon  form-control-feedback" id="spanFirstName"></span>
    </div>
</div>

<div class="form-group  has-feedback" id="divLastName">
    <label class="control-label col-sm-4" for="lastName">Last Name:</label>
    <div class="col-sm-8">
        <input type="text" class="form-control" id="lastName" maxlength="20" placeholder="Last Name" name="lastName">
        <span class="glyphicon  form-control-feedback" id="spanLastName"></span>
    </div>
</div>

<div class="form-group  has-feedback" id="divEmail">
    <label class="control-label col-sm-4" for="email">Email:</label>
    <div class="col-sm-8">
        <input type="email" class="form-control" id="email" maxlength="25" placeholder="Enter email" name="email">
        <span class="glyphicon  form-control-feedback" id="spanEmail"></span>
    </div>
</div>

<div class="form-group  has-feedback" id="divPhone">
    <label class="control-label col-sm-4" for="phone">Phone :</label>
    <div class="col-sm-8">
        <input type="number" class="form-control" id="phone" maxlength="20" placeholder="Enter Phone" name="phone">
        <span class="glyphicon  form-control-feedback" id="spanPhone"></span>
    </div>
</div>

@Html.HiddenFor(x => x.FromDate)
@Html.HiddenFor(x => x.ToDate)
@Html.HiddenFor(x => x.FromOrigin)
@Html.HiddenFor(x => x.ToDestination)
@Html.HiddenFor(x => x.Origin)
@Html.HiddenFor(x => x.SelectedValue)
